{
  "name": "lifemodel",
  "version": "0.1.0",
  "description": "Human-like proactive AI agent",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky"
  },
  "keywords": [
    "ai",
    "agent",
    "proactive",
    "human-like"
  ],
  "lint-staged": {
    "*.ts": "eslint --max-warnings 0",
    "*.{ts,json}": "prettier --check"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.10",
    "@types/semver": "^7.7.1",
    "@types/turndown": "^5.0.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.27",
    "@grammyjs/runner": "^2.0.3",
    "@openrouter/ai-sdk-provider": "^2.2.3",
    "ai": "^6.0.82",
    "cron-parser": "^5.5.0",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^5.3.4",
    "fastify": "^5.7.2",
    "grammy": "^1.39.3",
    "iconv-lite": "^0.7.2",
    "luxon": "^3.7.2",
    "node-html-parser": "^7.0.2",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "semver": "^7.7.3",
    "turndown": "^7.2.2",
    "zod": "^4.3.6"
  }
}
